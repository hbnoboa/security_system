    <table class="table table-striped table-bordered table-sm" >
    <thead>
      <tr>
        <th class="text-center">Local</th>
        <th class="text-center">Peça</th>
        <th class="text-center">Tipo</th>
        <th class="text-center">Nível</th>
        <th class="text-center">Quadrante</th>
        <th class="text-center">Medida</th>
        <th class="text-center">
          <%= link_to "Adicionar Dados", new_vehicle_nonconformity_path(@vehicle), class: "btn btn-primary" %>
        </th>
      </tr>
    </thead>
    <% @vehicle.nonconformities.each do |nonconformity| %>
    <tbody>
      <tr>
        <td class="text-center"><%= NonconformityLocal.find(nonconformity.nonconformityLocals).local %></td>
        <td class="text-center"><%= VehiclePart.find(nonconformity.vehicleParts).name %></td>
        <td class="text-center"><%= nonconformity.nonconformityTypes == "null" ? '' : NonconformityType.find(nonconformity.nonconformityTypes).nctype %></td>
        <td class="text-center"><%= NonconformityLevel.find(nonconformity.nonconformityLevels).level %></td>
        <td class="text-center"><%= nonconformity.quadrants == "null" ? '' : Quadrant.find(nonconformity.quadrants).option %></td>
        <td class="text-center"><%= Measure.find(nonconformity.measures).size %></td>


        <td class="justify-content-center d-flex">
          <%= link_to image_tag("see.png", :alt => "sair", size: "12x12", class:"img-fluid"), [@vehicle, nonconformity], class: "btn btn-link px-1" %>
        </td>
      </tr>
    </tbody>
    <% end %>
  </table> 
<br>